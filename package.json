{
  "name": "verivox-web-tests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .js,.vue,.ts --ignore-path .gitignore .",
    "cy:open": "cypress open",
    "cy:run": "cypress run --env allure=true",
    "allure:report:clear": "rimraf allure-results && rimraf allure-report",
    "allure:report:generate": "allure generate allure-results --clean -o allure-report",
    "allure:report:open": "allure open allure-report",
    "cy:run:report": "npm run allure:report:clear && npm run cy:run && npm run allure:report:generate",
    "cy:run:report:open": "npm run allure:report:clear && npm run cy:run && npm run allure:report:generate && npm run allure:report:open"
  },
  "author": "nabil",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "allure-commandline": "^2.30.0",
    "allure-cypress": "^3.0.0-beta.12",
    "babel-eslint": "^10.0.1",
    "cypress": "^13.14.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cucumber": "^2.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.0.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step-definitions/*.{js,ts}",
    "nonGlobalStepDefinitions": true
  }
}
